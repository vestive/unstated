{
  "name": "unstated-no-context-polyfill",
  "version": "1.0.0",
  "description": "Unstated, without the create-react-context polyfill",
  "main": "lib/unstated.js",
  "module": "lib/unstated.es.js",
  "types": "lib/unstated.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vestive/unstated.git"
  },
  "author": "James Kyle & Vestive",
  "license": "MIT",
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rm -rf lib",
    "build": "rollup -c && flow-copy-source src lib && cp src/unstated.d.ts lib/unstated.d.ts",
    "typecheck": "flow",
    "test": "jest",
    "format": "prettier --write **/*.{js,json,md}",
    "prepublish": "npm run clean && npm run build",
    "precommit": "lint-staged",
    "example": "parcel example/index.html",
    "typescript": "tsc -p tsconfig.json"
  },
  "peerDependencies": {
    "react": "^16.2.0"
  },
  "devDependencies": {
    "@types/react": "^16.0.36",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "flow-bin": "^0.64.0",
    "flow-copy-source": "^1.2.2",
    "husky": "^0.14.3",
    "jest": "^22.1.4",
    "jsdom": "^11.6.2",
    "lint-staged": "^6.1.0",
    "parcel-bundler": "^1.5.1",
    "prettier": "^1.10.2",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "rollup": "^0.55.3",
    "rollup-plugin-babel": "^3.0.3",
    "typescript": "^2.7.1"
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "testURL": "http://localhost/"
  },
  "bugs": {
    "url": "https://github.com/vestive/unstated/issues"
  },
  "homepage": "https://github.com/vestive/unstated#readme",
  "directories": {
    "example": "example",
    "lib": "lib"
  },
  "dependencies": {},
  "keywords": [
    "unstated"
  ]
}
